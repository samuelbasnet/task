class Vehicle {
    void drive() {
        System.out.println("Driving the vehicle");
    }
}

class Car extends Vehicle {
    void display() {
        System.out.println("Car details: Model - Tesla, Year - 2025");
    }
}

public class Main1 {
    public static void main(String[] args) {
        Car car = new Car();
        car.drive();
        car.display();
    }
}
